{
  "manifest_version": 2,

  "name": "SpareRoom helper",
  "description": "This extension adds pcm prices next to pw ones and add a gmaps link",
  "version": "0.2",

  "content_scripts": [
    {
      "matches": ["https://www.spareroom.co.uk/flatshare/*"],
      "css": ["spareroom-helper.css"],
      "js": ["jquery.js", "options.js", "spareroom-helper.js"]
    }
  ],

  "options_page": "options.html"
}
