{
  "manifest_version": 2,

  "name": "SpareRoom commute finder",
  "description": "This extension adds commute time to your set workplace to spareroom ads when possible",
  "version": "0.2",

  "content_scripts": [
    {
      "matches": ["http://www.spareroom.co.uk/flatshare/flatshare_detail.pl*"],
      "css": ["commute_finder.css"],
      "js": ["jquery.js", "commute_finder.js"]
    }
  ],
  "permissions": [
    "https://ajax.googleapis.com/"
  ]
}
